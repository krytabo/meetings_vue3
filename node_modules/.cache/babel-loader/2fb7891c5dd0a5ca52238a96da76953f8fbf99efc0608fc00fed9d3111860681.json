gs.forEach(item => {\n          this.$refs.multipleTables.toggleRowSelection(item, false);\n        });\n      });\n    },\n    //取消\n    memberListDialog_Cancel() {\n      this.dialogVisible = false;\n      this.$nextTick(() => {\n        this.tags.forEach(item => {\n          this.$refs.multipleTables.toggleRowSelection(item, false);\n        });\n      });\n    },\n    handleClose(tag) {\n      this.tags.splice(this.tags.indexOf(tag), 1);\n    },\n    handleInputConfirm() {\n      let inputValue = this.inputValue;\n      if (inputValue) {\n        this.tags.push({\n          name: inputValue,\n          id: GenNonDuplicateID(8),\n          department: \"\"\n        });\n      }\n      this.inputValue = \"\";\n    }\n  },\n  mounted() {\n    this.getApi();\n  }\n};","map":{"version":3,"mappings":";AA6BA,SAASA,iBAAgB,QAAS,gBAAgB;AAClD;AACA,SAASC,QAAO,QAAS,oBAAoB;AAC7C,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,UAAU,EAAE,CAAC,CAAC;EACdC,IAAI,GAAG;IACL,OAAO;MACLC,aAAa,EAAE,KAAK;MACpBC,KAAK,EAAE;QACLC,QAAQ,EAAE,CACR;UAAEC,QAAQ,EAAE,IAAI;UAAEC,OAAO,EAAE,SAAS;UAAEC,OAAO,EAAE;QAAO,CAAC,EACvD;UAAEC,GAAG,EAAE,CAAC;UAAEC,GAAG,EAAE,CAAC;UAAEH,OAAO,EAAE,eAAe;UAAEC,OAAO,EAAE;QAAO,CAAC,CAC9D;QACDG,QAAQ,EAAE,CAAC;UAAEL,QAAQ,EAAE,IAAI;UAAEC,OAAO,EAAE,SAAS;UAAEC,OAAO,EAAE;QAAS,CAAC;MACtE,CAAC;MACDI,IAAI,EAAE,CACJ;QACEC,EAAE,EAAE,OAAO;QACXC,UAAU,EAAE,KAAK;QACjBd,IAAI,EAAE;MACR,CAAC,EACD;QACEa,EAAE,EAAE,OAAO;QACXC,UAAU,EAAE,KAAK;QACjBd,IAAI,EAAE;MACR,CAAC,EACD;QACEa,EAAE,EAAE,OAAO;QACXC,UAAU,EAAE,KAAK;QACjBd,IAAI,EAAE;MACR,CAAC,CACF;MACDe,UAAU,EAAE,EAAE;MACdC,UAAU,EAAE,EAAE;MACdC,UAAU,EAAE;IACd,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP;IACAC,MAAM,GAAG;MACPpB,QAAQ,EAAE,CAACqB,IAAI,CAAEC,GAAG,IAAK;QACvB,IAAI,CAACL,UAAS,GAAIK,GAAG,CAACnB,IAAI;MAC5B,CAAC,CAAC;IACJ,CAAC;IACD;IACAoB,MAAM,CAACC,GAAG,EAAE;MACV,IAAI,CAACN,UAAS,GAAIO,IAAI,CAACC,KAAK,CAACD,IAAI,CAACE,SAAS,CAACH,GAAG,CAAC,CAAC;IACnD,CAAC;IACD;IACAI,gBAAgB,GAAG;MACjB,IAAI,CAACxB,aAAY,GAAI,IAAI;MACzB;MACA;;MAEA;MACA,IAAI,CAACyB,SAAS,CAAC,MAAM;QA