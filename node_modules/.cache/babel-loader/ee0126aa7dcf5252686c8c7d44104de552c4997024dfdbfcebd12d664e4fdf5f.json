{"ast":null,"code":"import { userList } from \"@/views/config/api\";\nexport default {\n  name: \"memberDialog\",\n  prop: {\n    inputName: String,\n    modelValue: {\n      type: String,\n      default: \"\"\n    },\n    dialogTitle: {\n      type: String,\n      default: \"标题\"\n    },\n    visible: {\n      type: Boolean,\n      default: false\n    },\n    popupWidth: {\n      type: String,\n      default: \"550px\"\n    }\n  },\n  emits: [\"dialogClose\", \"dialogSuccess\"],\n  data() {\n    return {\n      searchText: this.inputName,\n      memberList: [],\n      selected: null,\n      checkedDetail: []\n    };\n  },\n  mounted() {\n    userList().then(res => {\n      this.memberList = res.data;\n    });\n  },\n  methods: {\n    // table選擇框\n    select(rows) {\n      this.checkedDetai = rows;\n    },\n    // 確認按鈕\n    confirmButton() {\n      // this.$emit(\"childByValue\", this.searchText);\n      this.$emit(\"submitPopupData\");\n    },\n    // 取消按鈕\n    cancelButton() {\n      this.$emit(\"resetPopupData\");\n      this.$nextTick(() => {\n        this.editForm.countersign.forEach(item => {\n          this.$refs.multipleTables.toggleRowSelection(item, false);\n        });\n      });\n    }\n  },\n  watch: {\n    multipleTable() {\n      this.$nextTick(() => {\n        this.$refs.multipleTable.toggleRowSelection(this.memberList[0], true);\n      });\n    }\n  },\n  computed: {\n    dialogVisible: {\n      get() {\n        return this.visible;\n      },\n      set(val) {\n        this.$emit(\"updateVisible\", val);\n      }\n    }\n  }\n};","map":{"version":3,"mappings":"AAkCA,SAASA,QAAO,QAAS,oBAAoB;AAE7C,eAAe;EACbC,IAAI,EAAE,cAAc;EACpBC,IAAI,EAAE;IACJC,SAAS,EAAEC,MAAM;IACjBC,UAAU,EAAE;MACVC,IAAI,EAAEF,MAAM;MACZG,OAAO,EAAE;IACX,CAAC;IACDC,WAAW,EAAE;MACXF,IAAI,EAAEF,MAAM;MACZG,OAAO,EAAE;IACX,CAAC;IACDE,OAAO,EAAE;MACPH,IAAI,EAAEI,OAAO;MACbH,OAAO,EAAE;IACX,CAAC;IACDI,UAAU,EAAE;MACVL,IAAI,EAAEF,MAAM;MACZG,OAAO,EAAE;IACX;EACF,CAAC;EACDK,KAAK,EAAE,CAAC,aAAa,EAAE,eAA