-table-column>\n      </el-table>\n    </div>\n  </el-dialog>\n</template>\n\n<script>\nimport { userList } from \"@/views/config/api\";\n\nexport default {\n  name: \"memberDialog\",\n  data() {\n    return {\n      memberList: [],\n      selected: null,\n    };\n  },\n  mounted() {\n    userList().then((res) => {\n      this.memberList = res.data.memberList;\n    });\n  },\n  methods: {\n    select(rows, row) {\n      this.selected = rows.length && rows.indexOf(row) !== -1;\n      console.log(rows, 1);\n      console.log(row, 2);\n    },\n    selectAll(rows, row) {\n      console.log(row,rows);\n    },\n  },\n};\n</script>\n\n<style scoped></style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}