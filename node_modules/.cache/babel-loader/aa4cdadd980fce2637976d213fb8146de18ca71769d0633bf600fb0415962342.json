ect(rows, row) {\n      this.selected = rows.length && rows.indexOf(row) !== -1;\n      console.log(rows, 1);\n      console.log(row, 2);\n    },\n    selectAll(rows) {\n      console.log(rows);\n    },\n    sendSearch(e) {\n      this.$emit(\"update:modelValue\", e);\n    },\n  },\n};\n</script>\n\n<style scoped></style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}