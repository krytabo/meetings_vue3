bject.keys(dataNews).some(key => {\n            return String(dataNews[key]).indexOf(search.toUpperCase()) > -1;\n            //.indexOF()區分大小寫 .indexOf(.toUpperCase())不分大小寫 .toLowerCase()轉換小寫\n          });\n        });\n      }\n\n      return this.memberList;\n    }\n  }\n};","map":{"version":3,"mappings":";AAqCA,eAAe;EACbA,IAAI,EAAE,aAAa;EACnBC,UAAU,EAAE,CAAC,CAAC;EACdC,KAAK,EAAE;IACLC,YAAY,EAAE;MACZC,IAAI,EAAEC,MAAM;MACZC,OAAO,EAAE;IACX,CAAC;IACDC,KAAK,EAAE;MACLH,IAAI,EAAEI,KAAK;MACXF,OAAO,EAAE,MAAM;IACjB,CAAC;IACDG,UAAU,EAAE;MACVL,IAAI,EAAE,CAACC,MAAM,EAAEK,MAAM,CAAC;MACtBJ,OAAO,EAAE;IACX,CAAC;IACDK,QAAQ,EAAE;MACRP,IAAI,EAAEQ,OAAO;MACbN,OAAO,EAAE;IACX;EACF,CAAC;EACDO,IAAI,GAAG;IACL,OAAO;MACLC,GAAG,EAAE,EAAE;MACPC,gBAAgB,EAAE,KAAK;MACvBC,OAAO,EAAE,KAAK;MACdC,MAAM,EAAE,IAAI,CAACV,KAAK;MAClBW,YAAY,EAAE,EAAE;MAChBC,gBAAgB,EAAE,EAAE;MACpBC,iBAAiB,EAAE,EAAE;MACrBC,UAAU,EAAE,CACV;QACEC,EAAE,EAAE,OAAO;QACXC,UAAU,EAAE,MAAM;QAClBvB,IAAI,EAAE;MACR,CAAC,EACD;QACEsB,EAAE,EAAE,OAAO;QACXC,UAAU,EAAE,KAAK;QACjBvB,IAAI,EAAE;MACR,CAAC,EACD;QACEsB,EAAE,EAAE,OAAO;QACXC,UAAU,EAAE,KAAK;QACjBvB,IAAI,EAAE;MACR,CAAC,EACD;QACEsB,EAAE,EAAE,OAAO;QACXC,UAAU,EAAE,KAAK;QACjBvB,IAAI,EAAE;MACR,CAAC,CACF;MACDwB,aAAa,EAAE,EAAE;MACjBC,UAAU,EAAE;IACd,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,kBAAkB,GAAG;MACnB,IAAIF,UAAS,GAAI,IAAI,CAACA,UAAU;MAChC,IAAIA,UAAU,EAAE;QACd,IAAI,CAACR,MAAM,CAACW,IAAI,CAACH,UAAU,CAAC;MAC9B;MACA,IAAI,CAACA,UAAS,GAAI,EAAE;IACtB,CAAC;IACDI,WAAW,CAACf,GAAG,EAAE;MACf,IAAI,CAACG,MAAM,CAACa,MAAM,CAAC,IAAI,CAACb,MAAM,CAACc,OAAO,CAACjB,GAAG,CAAC,EAAE,CAAC,CAAC;IACjD,CAAC;IACDkB,uBAAuB,GAAG;MACxB,IAAIC,GAAE,GAAI,IAAI,CAACT,aAAa;MAC5B,IAAI,CAACU,KAAK,CAAC,mBAAmB,EAAED,GAAG,CAAC;MACpC,IAAI,CAAChB,MAAK,GAAIgB,GAAG;MACjB,IAAI,CAACE,KAAK,CAACnB,OAAO,CAACoB,IAAI,EAAE,EAAE;MAC3B;IACF,CAAC;;IACDC,OAAO,CAACC,CAAC,EAAE;MACT,IAAI,CAACJ,KAAK,CAAC,mBAAmB,EAAEI,CAAC,CAAC;IACpC,CAAC;IACDC,eAAe,CAACN,GAAG,EAAE;MACnB,KAAK,IAAIO,IAAI,CAAC,EAAEA,IAAIP,GAAG,CAACQ,MAAM,EAAED,CAAC,EAAE,EAAE;QACnC,IAAI,CAAChB